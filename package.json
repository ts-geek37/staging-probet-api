{
  "name": "probet-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "db": "graphile-migrate",
    "setup:db": "node -r dotenv/config ./scripts/setup_db.js"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "pg": "^8.16.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}